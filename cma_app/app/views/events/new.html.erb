<%= form_for @event do |f| %>
    <%= f.date_field :date, placeholder: 'Date' %><br>
    Alcahol Served ? <%= f.select :alcohol_served, [['Yes', true], ['No', false]]%><br>
	<%= f.collection_select :venue_id, Venue.all, :id, :name %><br>
	<br>
	<select name="band_id">
		<%= @bands.each do |x| %>
			<option value="<%= x.id %>"><%= x.name %></option>
		<% end %>
	</select>
    <%= f.submit %>
<% end %>
<%=@venue.name%>