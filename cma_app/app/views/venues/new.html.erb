<%= form_for @venue do |f| %>
    <%= f.text_field :name, placeholder: 'Name' %><br>
    <%= f.text_field :city, placeholder: 'City' %><br>
    <%= f.text_field :state, placeholder: 'State' %><br>
    Family Friendly ? <%= f.select :family_friendly, [['Yes', true], ['No', false]]%><br>
	<br>
    <%= f.submit %>
<% end %>